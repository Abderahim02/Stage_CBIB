process {
    withName:runMusic {
        container = 'csangara/spade_music:latest'
    }
    withName:runSpotlight {
        container = 'csangara/spade_spotlight:latest'
    }
    withName:runRCTD {
        container = 'csangara/spade_rctd:latest'
    }
}
docker {
    enabled = true
    runOptions = "--mount type=bind,source=/mnt/d/spade-benchmark/,target=/mnt/d/spade-benchmark/"
}
