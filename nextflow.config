profiles {
    local_env {
        process.executor = 'local'
        workDir = '/mnt/d/spade-benchmark/scripts/work'

    }

    local_docker {
        process.executor = 'local'
        docker.enabled = true
        docker.runOptions = "--mount type=bind,source=/mnt/d/,target=/mnt/d/"
        workDir = '/mnt/d/spade-benchmark/scripts/work'
    }

    prism {
        process.executor = "sge"
        docker.enabled = true
        workDir = "~/spade-benchmark/scripts/work"
    }

}