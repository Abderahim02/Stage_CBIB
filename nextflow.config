workDir = '/mnt/d/spade-benchmark/scripts/work'

profiles {
    local_env {
        process.executor = 'local'

    }

    local_docker {
        process.executor = 'local'
        docker.enabled = true
        docker.runOptions = "--mount type=bind,source=/mnt/d/,target=/mnt/d/"
    }

    prism {
        process.executor = "sge"
        docker.enabled = true
    }

}